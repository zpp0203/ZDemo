<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义start view-->
    <declare-styleable name="custom_start_view">
        <attr name="start_number" format="integer"/>
        <attr name="rating" format="float"/>
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="start_size" format="dimension"/>
        <attr name="full_image" format="reference"/>
        <attr name="haif_image" format="reference"/>
        <attr name="no_image" format="reference"/>
        <attr name="start_margin" format="dimension"/>
    </declare-styleable>
</resources>
